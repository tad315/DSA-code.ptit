#include <bits/stdc++.h>
using namespace std;
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);

int main(){
    int t,n;
    cin >> t;
    string s;
    while (t--){
        cin >> n >> s;
        cout << n << " ";
        if (next_permutation(s.begin(), s.end())) cout << s << endl;
        else cout << "BIGGEST" << endl;
    }
    return 0;
}
