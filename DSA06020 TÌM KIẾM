#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);

int main() {
    fast;
    int t;
    cin >> t;
    while(t--){
        int n,k;
        cin >> n >> k;
        int a[n];
        int check = 0;
        for(int i=0; i<n; ++i)
        {
            cin >> a[i];
            if(a[i] == k) check = 1;
        }
        if(check == 1) cout << 1 << endl;
        else cout << -1 << endl;
    }
}
