#include <bits/stdc++.h>
using namespace std;
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define ll long long
const ll mod = 1e9 + 7;

int main() {
    fast;
    int t; 
    cin >> t; 
    while (t--) {
        int n; 
        cin >> n;
        vector <int> a(n);
        for (int &i : a) cin >> i;
        sort (a.begin(), a.end());
        for (int i:a) cout << i << " ";
        cout << endl;
    }
    return 0;
}
