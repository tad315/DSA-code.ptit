#include <bits/stdc++.h>
using namespace std;
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);

int main() {
    fast;
    int t;
    cin >> t;
    while (t--) {
        int n,s,m;
        cin >> n >> s >> m;
        int f = m*s;
        int d = s - (s/7);
        if (m>n || f > d*n) cout << -1 << endl;
        else cout << (f+n-1)/n << endl;
    }
    return 0;
}
