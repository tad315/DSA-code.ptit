#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);

int main() {
    fast;
    int t;
    cin >> t;
    while(t--){
        int n,m;
        cin >> n >> m;
        long long a[n], b[m];
        for(long long &x : a) cin >> x;
        sort(a, a+n);
        for(long long &x : b) cin >> x;
        sort(b, b+m);
        cout << a[n-1] * b[0] << endl;
    }
}
