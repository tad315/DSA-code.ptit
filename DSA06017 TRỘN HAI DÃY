#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
const int mod = 1e9 + 7;

int main (){
    int t;
    cin >> t;
    int n, m;
    while (t--){
        cin >> n >> m;
        int a[n+1], b[m+1];
        vector <int> s;
        for (int i = 0; i<n; i++){
            cin >> a[i];
            s.push_back(a[i]);
        }
        for (int i = 0; i<m; i++){
            cin >> b[i];
            s.push_back(b[i]);
        }
        sort (s.begin(), s.end());
        for (int &x: s) cout << x << " ";
        cout << endl;
    }
}
