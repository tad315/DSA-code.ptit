#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);

int main() {
    fast;
    int t;
    cin >> t;
    int n;
    while (t--){
        cin >> n;
       vector<int> a(n);
        for (int i= 0; i<n; i++){
            cin >> a[i];
        }
        sort(a.begin(), a.end());
        for (int &x: a) cout << x << " ";
        cout << endl;
    }
    return 0;
}
