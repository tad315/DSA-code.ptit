#include <bits/stdc++.h>
using namespace std;
#define boost ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0)
#define ll long long
#define endl '\n'
#define all(x) x.begin(), x.end()
#define pb push_back
#define mp make_pair
#define fi first
#define se second

int main()
{
    boost;
    int n, k;
    cin >> n >> k;
    string s[n];
    for (int i = 0; i < n; ++i) cin >> s[i];
    string tmp = "";
    for (int i = 0; i < n; ++i) tmp += char(i + '0');
    int res = 1e9;
    do
    {
        int minN = 1e9, maxN = -1;
        for (int i = 0; i < n; ++i)
        {
            string t = "";
            for (int j = 0; j < k; ++j)
            {
                t += s[i][tmp[j] - '0'];
            }
            int v = stoi(t);
            minN = min(minN, v);
            maxN = max(maxN, v);
        }
        res = min(res, maxN - minN);
    } while (next_permutation(all(tmp)));
    cout << res;
    return 0;
}
