#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);

int main() {
    fast;
    int t, n;
    cin >> t;
    while (t--) {
        int a[1005];
        cin >> n;
        for (int i = 0; i<n; ++i) cin >> a[i];
        next_permutation(a, a+n);
        for (int i = 0; i<n; ++i) cout << a[i] << " ";
        cout << endl;
    }
    return 0;
}
